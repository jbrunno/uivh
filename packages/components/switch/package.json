{
  "name": "@vhsys-ui/switch",
  "version": "0.0.64",
  "description": "A switch is similar to a checkbox, but represents on/off values as opposed to selection.",
  "keywords": [
    "switch"
  ],

  "homepage": "https://vhsys.com.br",

  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],

  "repository": {
    "type": "git",
    "url": "git@git.vhsys.com.br:front/frontend-core.git",
    "directory": "packages/components/switch"
  },

  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "pnpm build:fast --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "@vhsys-ui/theme": ">=2.1.0",
    "@vhsys-ui/system": ">=2.0.0"
  },
  "dependencies": {
    "@vhsys-ui/shared-utils": "workspace:*",
    "@vhsys-ui/react-utils": "workspace:*",
    "@vhsys-ui/use-aria-press": "workspace:*",
    "@react-aria/focus": "^3.14.3",
    "@react-aria/interactions": "^3.19.1",
    "@react-aria/switch": "^3.5.6",
    "@react-aria/utils": "^3.21.1",
    "@react-aria/visually-hidden": "^3.8.6",
    "@react-stately/toggle": "^3.6.3",
    "@react-types/shared": "^3.21.0"
  },
  "devDependencies": {
    "@vhsys-ui/theme": "workspace:*",
    "@vhsys-ui/system": "workspace:*",
    "@vhsys-ui/shared-icons": "workspace:*",
    "clean-package": "2.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "clean-package": "../../../clean-package.config.json"
}
