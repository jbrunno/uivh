  },
  "dependencies": {
    "@vhsys-ui/shared-utils": "workspace:*",
    "@vhsys-ui/react-utils": "workspace:*",
    "@vhsys-ui/use-image": "workspace:*"
  },
  "devDependencies": {
    "@vhsys-ui/theme": "workspace:*",
    "@vhsys-ui/system": "workspace:*",
    "clean-package": "2.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "clean-package": "../../../clean-package.config.json"
