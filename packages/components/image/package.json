{
  "name": "@vhsys-ui/image",
  "version": "2.0.24",
  "description": "A simple image component",
  "keywords": [
    "image"
  ],

  "homepage": "https://vhsys.com.br",

  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],

  "repository": {
