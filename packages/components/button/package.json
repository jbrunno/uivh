{
  "name": "@vhsys-ui/button",
  "version": "2.0.26",
  "description": "Buttons allow users to perform actions and choose with a single tap.",
  "keywords": [
    "button"
  ],

  "homepage": "https://vhsys.com.br",

  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
