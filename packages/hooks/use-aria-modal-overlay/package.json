{
  "name": "@vhsys-ui/use-aria-modal-overlay",
  "version": "0.0.64",
  "description": "A custom implementation of react aria modal overlay, this removes the prevent scroll",
  "keywords": [
    "use-aria-modal-overlay"
  ],

  "homepage": "https://vhsys.com.br",

  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],

  "repository": {
    "type": "git",
    "url": "git@git.vhsys.com.br:front/frontend-core.git",
    "directory": "packages/hooks/use-aria-modal-overlay"
  },

  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "pnpm build:fast --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "@react-aria/overlays": "^3.18.1",
    "@react-stately/overlays": "^3.6.3",
    "@react-types/shared": "^3.21.0",
    "@react-aria/utils": "^3.21.1"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "clean-package": "2.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "clean-package": "../../../clean-package.config.json",
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ]
  }
}
